    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-formatter-codeframe": "^7.32.1",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-import": "^2.31.0",
