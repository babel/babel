    "eslint-plugin-n": "^16.5.0",
    "eslint-plugin-prettier": "^5.1.0",
    "glob": "^10.3.10",
    "gulp": "^4.0.2",
    "gulp-filter": "^7.0.0",
    "gulp-plumber": "^1.2.1",
    "husky": "^8.0.3",
    "import-meta-resolve": "^4.0.0",
    "is-ci": "^3.0.1",
    "jest": "^30.0.0-alpha.2",
