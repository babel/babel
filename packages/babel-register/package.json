{
  "name": "@babel/register",
  "version": "7.10.5",
  "description": "babel require hook",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/babel/babel.git",
    "directory": "packages/babel-register"
  },
  "author": "Sebastian McKenzie <sebmck@gmail.com>",
  "main": "lib/index.js",
  "module": "lib/index.module.js",
  "exports": {
    ".": {
      "import": "./lib/index.module.js",
      "default": "./lib/index.js"
    },
    "./browser": {
      "import": "./lib/browser.module.js",
      "default": "./lib/browser.js"
    },
    "./cache": {
      "import": "./lib/cache.module.js",
      "default": "./lib/cache.js"
    },
    "./node": {
      "import": "./lib/node.module.js",
      "default": "./lib/node.js"
    }
  },
  "browser": {
    "./lib/node.js": "./lib/browser.js",
    "./lib/node.module.js": "./lib/browser.module.js"
  },
  "dependencies": {
    "find-cache-dir": "^2.0.0",
    "lodash": "^4.17.19",
    "make-dir": "^2.1.0",
    "pirates": "^4.0.0",
    "source-map-support": "^0.5.16"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0-0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "browserify": "16.5.0",
    "default-require-extensions": "^2.0.0"
  }
}
